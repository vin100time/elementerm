{
  "name": "elementerm",
  "version": "0.1.0",
  "description": "The invisible orchestrator for Claude Code sessions. See everything. Change nothing. Orchestrate everything.",
  "type": "module",
  "bin": {
    "elementerm": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "claude-code",
    "orchestrator",
    "terminal",
    "dashboard",
    "tui",
    "sessions",
    "worktree",
    "daemon"
  ],
  "author": "Vince",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "ink": "^6.8.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
